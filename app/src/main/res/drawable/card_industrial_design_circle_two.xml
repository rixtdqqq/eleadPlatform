<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval" >

    <!-- 填充颜色 -->
    <solid android:color="#F3F3F3" ></solid>

    <!--线的宽度，颜色灰色-->
    <stroke android:width="2dp" android:color="#F3F3F3"></stroke>

    <!-- 矩形的圆角半径 -->
    <corners android:radius="0.05dp" />
    <size android:height="15dp" android:width="15dp"></size>
</shape>